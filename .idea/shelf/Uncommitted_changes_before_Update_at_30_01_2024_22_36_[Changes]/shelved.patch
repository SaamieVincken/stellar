-        //  */
-        // if(hue > 60 && hue < 180){
-        //     lightness = new Decimal(95);
-        // }
-        return [hue, saturation, lightness];
-    }
-    else{
-        return [0, new Decimal(0), new Decimal(0)]; // black
-    }}
-}
